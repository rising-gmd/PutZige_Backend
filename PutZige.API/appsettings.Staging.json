{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "LoggingSettings": {
    "Environment": "Staging",
    "EnableSeq": false
  },
  "DatabaseSettings": {
    "ConnectionString": "Server=staging-server;Database=PutZige_Staging;User Id=staging;Password=staging;",
    "MaxRetryCount": 3,
    "CommandTimeout": 30,
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false
  }
}
